{
  "compilerOptions": {
    "inlineSourceMap": false,
    "inlineSources": false,
    "listFiles": true,
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "build",
    "preserveConstEnums": false,
    "removeComments": true,
    "rootDir": "src/server",
    "sourceMap": false,
    "target": "es6",
    "stripInternal": true
  },
  "exclude": [
    ".vscode",
    "_gulp",
    "node_modules",
    "typings"
  ]
}
