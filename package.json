{
  "name": "super-node-starter",
  "version": "3.0.0",
  "description": "a simple starter project for NodeJS Express apps",
  "main": "build/server/web.js",
  "engines": {
    "node": "6.x"
  },
  "peerDependencies": {
    "typescript": "1.8.10"
  },
  "scripts": {
    "preinstall": "npm i -g sequelize-cli",
    "migration": "sequelize db:migrate",
    "tsc": "tsc",
    "tsc:dev": "tsc --sourcemap",
    "prestart": "npm run migration && tsc",
    "start": "heroku local web",
    "deploy": "docker-compose up -d --remove-orphans --build && docker-compose logs -f",
    "develop": "heroku local tsc,web --procfile=Procfile_DV",
    "coverage": "heroku local cover --procfile=Procfile_DV",
    "debug": "node-debug bin/src/server/web.js",
    "pretest": "npm run prestart",
    "test": "mocha __tests__/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stephn-r/super-node-starter.git"
  },
  "author": "Stephen Rodriguez <steprodriguez10@gmail.com>",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/stephn-r/super-node-starter/issues"
  },
  "homepage": "https://github.com/stephn-r/super-node-starter#readme",
  "private": true,
  "dependencies": {
    "babel-cli": "6.11.4",
    "babel-preset-es2015": "6.13.2",
    "babel-register": "6.11.6",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "1.13.3",
    "checkit": "0.6.0",
    "compression": "1.5.2",
    "connect-redis": "3.0.2",
    "cookie-parser": "1.4.3",
    "cors": "2.7.1",
    "dotenv": "2.0.0",
    "express": "4.14.0",
    "express-graphql": "0.5.3",
    "express-session": "1.13.0",
    "graphql": "0.6.2",
    "graphql-custom-types": "0.6.0",
    "graphql-relay": "0.4.2",
    "graphql-sequelize": "2.4.0",
    "helmet": "2.0.0",
    "hpp": "0.2.1",
    "lodash": "3.10.1",
    "moment": "2.11.2",
    "node-uuid": "1.4.7",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "pg": "4.4.1",
    "sequelize": "3.24.1",
    "serve-static": "1.10.2",
    "typescript": "1.8.10",
    "typings": "1.3.3",
    "winston": "1.0.1"
  },
  "devDependencies": {
    "babel-eslint": "6.1.2",
    "chai": "3.5.0",
    "graphql-tools": "0.6.4",
    "gulp": "3.9.0",
    "gulp-eslint": "3.0.1",
    "gulp-load-plugins": "0.10.0",
    "gulp-nsp": "2.2.1",
    "gulp-util": "3.0.6",
    "jsonschema": "1.1.0",
    "require-dir": "0.3.0",
    "superagent": "2.2.0",
    "supertest": "2.0.0"
  }
}
