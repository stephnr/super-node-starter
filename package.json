{
  "name": "super-node-starter",
  "version": "1.0.0",
  "description": "a simple starter project for NodeJS Express apps",
  "main": "server/web.js",
  "engine-strict": true,
  "engines": {
    "node": ">=5.2.0"
  },
  "scripts": {
    "start": "foreman start -e .env -f Procfile_DV --port 8080",
    "heroku_start": "knex migrate:latest && node server/web.js",
    "test": "echo \"Error: no tests specified\"",
    "migrate": "knex migrate:latest",
    "seed": "knex seed:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stephn-r/super-node-starter.git"
  },
  "author": "Stephen Rodriguez <steprodriguez10@gmail.com>",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/stephn-r/super-node-starter/issues"
  },
  "homepage": "https://github.com/stephn-r/super-node-starter#readme",
  "private": true,
  "dependencies": {
    "body-parser": "1.13.3",
    "compression": "1.5.2",
    "cookie-parser": "1.3.5",
    "cookie-session": "1.2.0",
    "cors": "2.7.1",
    "cron": "1.1.0",
    "errorhandler": "1.4.2",
    "express": "4.13.3",
    "foreman": "1.4.1",
    "knex": "0.9.0",
    "lodash": "3.10.1",
    "method-override": "2.3.5",
    "passport": "0.3.2",
    "pg": "4.4.1",
    "winston": "1.0.1"
  },
  "devDependencies": {
    "babel": "5.8.23",
    "babel-core": "6.3.17",
    "babel-preset-es2015": "6.3.13",
    "gulp": "3.9.0",
    "gulp-babel": "6.1.1",
    "gulp-count": "0.1.0",
    "gulp-eslint": "1.1.1",
    "gulp-jsdoc": "0.1.5",
    "gulp-load-plugins": "0.10.0",
    "gulp-nsp": "2.2.1",
    "gulp-util": "3.0.6",
    "jsdoc": "3.4.0",
    "require-dir": "0.3.0"
  }
}
