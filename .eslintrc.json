{
  "ecmaFeatures": {
    "arrowFunctions": true,
    "binaryLiterals": false,
    "blockBindings": true,
    "classes": false,
    "defaultParams": false,
    "destructuring": false,
    "forOf": true,
    "generators": true,
    "modules": true,
    "objectLiteralComputedProperties": false,
    "objectLiteralDuplicateProperties": false,
    "objectLiteralShorthandMethods": false,
    "objectLiteralShorthandProperties": false,
    "octalLiterals": false,
    "regexUFlag": false,
    "regexYFlag": false,
    "restParams": false,
    "spread": true,
    "templateStrings": true,
    "unicodeCodePointEscapes": false,
    "globalReturn": false,
    "jsx": false
  },

  "parser": "babel-eslint",

  "env": {
    "browser": true,
    "node": true,
    "commonjs": false,
    "worker": false,
    "amd": true,
    "mocha": false,
    "jasmine": true,
    "jest": true,
    "phantomjs": false,
    "protractor": false,
    "qunit": false,
    "jquery": false,
    "prototypejs": false,
    "shelljs": false,
    "meteor": false,
    "mongo": false,
    "nashorn": false,
    "serviceworker": false,
    "embertest": false,
    "webextensions": false,
    "es6": true
  },

  "globals": {
    "angular": true
  },

  "plugins": [],

  "rules": {
    "comma-dangle": 0,
    "no-cond-assign": [ 2, "always" ],
    "no-console": 1,
    "no-constant-condition": 0,
    "no-control-regex": 0,
    "no-debugger": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 0,
    "no-empty-character-class": 0,
    "no-empty": 1,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 1,
    "no-extra-parens": [0, "all"],
    "no-extra-semi": 1,
    "no-func-assign": 2,
    "no-inner-declarations": [0, "functions"],
    "no-invalid-regexp": 1,
    "no-irregular-whitespace": 1,
    "no-negated-in-lhs": 1,
    "no-obj-calls": 1,
    "no-regex-spaces": 2,
    "no-sparse-arrays": 2,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "use-isnan": 2,
    "valid-jsdoc":  [1, {
      "requireReturn": false,
      "requireParamDescription": true,
      "requireReturnDescription": true,
      "matchDescription": "",
      "requireReturnType": true
    }],
    "valid-typeof": 2,



    "accessor-pairs": [2, {
      "getWithoutSet": true
    }],
    "block-scoped-var": 2,
    "complexity": [2, 7],
    "consistent-return": 0,
    "curly": [0, "all"],
    "default-case": 2,
    "dot-location": [1, "property"],
    "dot-notation": [1, {
      "allowKeywords": true,
      "allowPattern": ""
    }],
    "eqeqeq": [2, "smart"],
    "guard-for-in": 2,
    "no-alert": 2,
    "no-caller": 2,
    "no-case-declarations": 2,
    "no-div-regex": 0,
    "no-else-return": 0,
    "no-labels": 2,
    "no-empty-pattern": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 0,
    "no-implied-eval": 2,
    "no-implicit-coercion": [2, {
      "boolean": true,
      "number": true,
      "string": true
    }],
    "no-invalid-this": 2,
    "no-iterator": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-magic-numbers": [0, {
      "enforceConst": true,
      "detectObjects": false
    }],
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-new-func": 2,
    "no-new-wrappers": 1,
    "no-new": 1,
    "no-octal-escape": 2,
    "no-octal": 2,
    "no-param-reassign": 0,
    "no-process-env": 0,
    "no-proto": 2,
    "no-redeclare": [1, {
      "builtinGlobals": true
    }],
    "no-return-assign": [2, "always"],
    "no-script-url": 1,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-throw-literal": 1,
    "no-unused-expressions": [0, {
      "allowShortCircuit": true,
      "allowTernary": true
    }],
    "no-useless-call": 1,
    "no-useless-concat": 2,
    "no-void": 1,
    "no-warning-comments": [0, {
      "terms": ["todo", "fixme", "xxx"],
      "location": "start"
    }],
    "no-with": 2,
    "radix": [2, "always"],
    "vars-on-top": 2,
    "wrap-iife": 0,
    "yoda": [2, "never"],

    "strict": 0,

    "init-declarations": 0,
    "no-catch-shadow": 2,
    "no-delete-var": 2,
    "no-label-var": 2,
    "no-shadow-restricted-names": 2,
    "no-shadow": 2,
    "no-undef-init": 2,
    "no-undef": [2, {
      "typeof": true
    }],
    "no-undefined": 1,
    "no-unused-vars": [1, {
      "vars": "all",
      "args": "none"
    }],
    "no-use-before-define": [1, "nofunc"],


    "callback-return": [1, ["callback", "cb", "next"]],
    "global-require": 0,
    "handle-callback-err": 2,
    "no-mixed-requires": 1,
    "no-new-require": 2,
    "no-path-concat": 0,
    "no-process-exit": 2,
    "no-restricted-modules": [2, ""],
    "no-sync": 0,

    "array-bracket-spacing": [1, "always"],
    "block-spacing": [1, "always"],
    "brace-style": [1, "1tbs", { "allowSingleLine": true }],
    "camelcase": [0, {
      "properties": "always"
    }],
    "comma-spacing": [1, {
      "before": false,
      "after": true
    }],
    "comma-style": [1, "last"],
    "computed-property-spacing": [1, "always"],
    "consistent-this": 1,
    "eol-last": 2,
    "func-style": [1, "declaration", {
      "allowArrowFunctions": true
    }],
    "func-names": 0,
    "id-length": [0, {
      "min": 3,
      "max": 10,
      "properties": "never",
      "exceptions": ["x"]
    }],
    "id-match": [0, "^[a-z]+([A-Z][a-z]+)*$", {
      "properties": false
    }],
    "indent": [1, 2],
    "jsx-quotes": 0,
    "key-spacing": [1, {
      "beforeColon": false,
      "afterColon": true,
      "mode": "minimum",
      "align": "value"
    }],
    "linebreak-style": 0,
    "lines-around-comment": 0,
    "max-depth": [0, 10],
    "max-len": [0, 80, 4, {
      "ignoreComments": true,
      "ignoreUrls": true,
      "ignorePattern": "^\\s*var\\s.+=\\s*require\\s*\\("
    }],
    "max-nested-callbacks": [0, 3],
    "max-params": [1, 8],
    "max-statements": 0,
    "new-cap": [2, {
      "capIsNew": false,
      "newIsCapExceptions": [],
      "capIsNewExceptions": []
    }],
    "new-parens": 2,
    "newline-after-var": [0, "always"],
    "no-array-constructor": 1,
    "no-bitwise": 2,
    "no-continue": 1,
    "no-inline-comments": 2,
    "no-lonely-if": 2,
    "no-mixed-spaces-and-tabs": [2, "smart-tabs"],
    "no-multiple-empty-lines": [0, {
      "max": 3,
      "maxEOF": 1
    }],
    "no-negated-condition": 0,
    "no-nested-ternary": 2,
    "no-new-object": 1,
    "no-plusplus": 0,
    "no-restricted-syntax": [0, "FunctionExpression", "WithStatement"],
    "no-spaced-func": 2,
    "no-ternary": 0,
    "no-trailing-spaces": [1, {
      "skipBlankLines": true
    }],
    "no-underscore-dangle": [0, {}],
    "no-unneeded-ternary": 1,
    "object-curly-spacing": [1, "always"],
    "one-var": 0,
    "operator-assignment": [2, "always"],
    "operator-linebreak": [1, "before", { "overrides": { "?": "after" } }],
    "padded-blocks": [0, "never"],
    "quote-props": [0, "consistent-as-needed"],
    "quotes": [2, "single"],
    "require-jsdoc": [1, {
      "require": {
        "FunctionDeclaration": true,
        "MethodDefinition": true,
        "ClassDeclaration": true
      }
    }],
    "semi-spacing": [2, {
      "before": false,
      "after": true
    }],
    "semi": [2, "always"],
    "sort-vars": [1, {
      "ignoreCase": true
    }],
    "space-after-keywords": 0,
    "space-before-blocks": [0, {
      "functions": "never",
      "keywords": "always"
    }],
    "space-before-function-paren": [0, "never"],
    "space-in-parens": [0, "always"],
    "space-infix-ops": [0, {
      "int32Hint": false
    }],
    "space-unary-ops": [0, {
      "words": true,
      "nonwords": false
    }],
    "spaced-comment": 0,
    "wrap-regex": 0,

    "arrow-body-style": [0, "as-needed"],
    "arrow-parens": [1, "as-needed"],
    "arrow-spacing": [1, { "before": true, "after": true }],
    "constructor-super": 1,
    "generator-star-spacing": [2, {
      "before": false,
      "after": true
    }],
    "no-arrow-condition": 0,
    "no-class-assign": 2,
    "no-const-assign": 2,
    "no-dupe-class-members": 2,
    "no-this-before-super": 2,
    "no-var": 2,
    "object-shorthand": 0,
    "prefer-arrow-callback": 2,
    "prefer-const": 0,
    "prefer-reflect": 2,
    "prefer-spread": 2,
    "prefer-template": 2,
    "require-yield": 2
  }
}
